import javafx.stage.Stage;

public class SceneGraphApp extends Application{

	@Override
	public void start(Stage primaryStage) throws Exception{
		HBox hBox = new HBox();
		hBox.setAlignment(Pos.CENTER);
		hBox.getChildren().add(new Button("button 1"));
		
	}
	
}
